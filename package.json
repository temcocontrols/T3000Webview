    "@uppy/drag-drop": "^3.0.1",
    "@uppy/file-input": "^3.0.4",
    "@uppy/image-editor": "^2.1.0",
    "@uppy/progress-bar": "^3.0.1",
    "@uppy/vue": "^1.0.1",
    "ag-grid-community": "^31.0.1",
    "ag-grid-enterprise": "^31.0.1",
    "ag-grid-vue3": "^31.0.1",
    "anychart": "^8.11.0",
    "dotenv": "^16.4.4",
